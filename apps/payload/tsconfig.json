{
  "type": "module",
  "compilerOptions": {
    "target": "ES2022",
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "skipLibCheck": true
  },

  "resolveJsonModule": true,
  "esModuleInterop": true,
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
