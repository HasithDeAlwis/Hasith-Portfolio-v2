{
  "name": "@hasith/portfolio",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "dev:website": "astro dev --root ./apps/website",
    "start:website": "astro dev --root ./apps/website",
    "build:website": "astro check --root ./apps/website && astro build --root ./apps/website",
    "preview:website": "astro preview --root ./apps/website",
    "e2e:website": "pnpm exec playwright test --config=./apps/website-e2e/playwright.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "astro": "astro",
    "unit:website": "vitest --config ./apps/website/vitest.config.ts",
    "prepare": "if [ \"$NODE_ENV\" = \"development\" ]; then husky install; fi",
    "storybook": "storybook dev -p 6006 --config-dir ./apps/storybook/.storybook ",
    "build-storybook": "storybook build --config-dir ./apps/storybook/.storybook "
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/svelte": "^5.7.0",
    "@astrojs/tailwind": "^5.1.0",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/lib-storage": "^3.658.1",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-postgres": "^0.8.6",
    "@payloadcms/plugin-cloud-storage": "^1.1.3",
    "@payloadcms/richtext-slate": "^1.5.2",
    "astro": "^4.14.2",
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "express": "^4.21.0",
    "gsap": "^3.12.5",
    "payload": "^2.28.0",
    "s3": "link:@payloadcms/plugin-cloud-storage/s3",
    "sharp": "^0.33.5",
    "split-type": "^0.3.4",
    "svelte": "^4.2.19",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@astrojs/ts-plugin": "^1.10.2",
    "@chromatic-com/storybook": "^1.9.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@playwright/test": "^1.46.1",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-svelte-csf": "^4.1.7",
    "@storybook/blocks": "^8.2.9",
    "@storybook/builder-vite": "^8.2.9",
    "@storybook/svelte": "^8.2.9",
    "@storybook/svelte-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/gsap": "^3.0.0",
    "@types/node": "^22.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.44.0",
    "husky": "^9.1.5",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.45",
    "storybook": "^8.2.9",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*.{js,ts,astro,mjs,cjs,mts,mjs}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
